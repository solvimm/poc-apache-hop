        <jsScript_script>//Script here
if (count == 1) {
   id_pessoa_x = id_pessoa;
   nome_x = nome;
   url_linkedin_x = url_linkedin;
   url_linkedin_id_x = url_linkedin_id;
   empregado_x = empregado;
   atualizacao_x = atualizacao;
   observacoes_x = observacoes;
   len_nome_x = nome.length;
}

// Garante o nome mais completo
len_nome = nome.length 

if(len_nome > len_nome_x){
    nome_x = nome;
    len_nome_x = len_nome;
}

if(len_nome &lt; len_nome_x){
    nome = nome_x;
    len_nome = len_nome_x;
}
// Coloca com iniciais maiúsculas
excecoes = ["de", "da", "do", "das", "dos", "e"];

nome = nome
  .toLowerCase()
  .split(" ")
  .map((palavra, i) => 
    excecoes.includes(palavra) &amp;&amp; i !== 0
      ? palavra
      : palavra.charAt(0).toUpperCase() + palavra.slice(1)
  )
  .join(" ");

// 1️⃣ Remove emojis
nome = nome.replace(/([\uD83C-\uDBFF][\uDC00-\uDFFF]|\uFE0F|\u200D)/g, "");

// 2️⃣ Remove texto entre parênteses (normais ou cheios)
nome = nome.replace(/[（(].*?[）)]/g, "");

// 3️⃣ Remove espaços extras
nome = nome.trim().replace(/\s+/g, " ");

// 4️⃣ Coloca iniciais maiúsculas, exceto conectores comuns
nome = nome
  .toLowerCase()
  .split(" ")
  .map((palavra, i) => {
    const excecoes = ["de", "da", "do", "das", "dos", "e"];
    return excecoes.includes(palavra) &amp;&amp; i !== 0
      ? palavra
      : palavra.charAt(0).toUpperCase() + palavra.slice(1);
  })
  .join(" ");                         // remove espaços extras
// fim


if(url_linkedin.length > url_linkedin_x.length){
    url_linkedin_x = url_linkedin
}else{
    url_linkedin = url_linkedin_x
}

// Trata Linkedin
if(url_linkedin &amp;&amp; url_linkedin != 'N/I' &amp;&amp; url_linkedin != ''){
  url = url_linkedin.trim();
  // Remove protocolo duplicado e coloca tudo em lowercase
  url = url.replace(/^https?:\/\//i, '').toLowerCase();
  // Remove qualquer coisa antes de linkedin.com
  liIndex = url.lastIndexOf('linkedin.com');
  if (liIndex != -1) {
    url = url.slice(liIndex + 'linkedin.com'.length);
  }

  // Remove query params e barras extras
  url = url.split(/[\/?&amp;]/).filter(Boolean); // cria array de segmentos
 
  id = null;
  // Procura o segmento "in" se existir
  inPos = url.indexOf('in');
  if (inPos !== -1 &amp;&amp; inPos &lt; url.length - 1) {
    id = url[inPos + 1]; // pega o segmento logo após "in"
  } else {
    // Se não tiver "in", pega o último segmento
    id = url[url.length - 1];
  }
  url_linkedin = `https://www.linkedin.com/in/${id}`;
  url_linkedin_id = id;
}else{
  url_linkedin = 'N/I';
  url_linkedin_id = 'N/I';
}


</jsScript_script>
