    <limit>0</limit>
    <sql>SELECT * FROM (
SELECT DISTINCT
    upper(case when ec.cargo is null or ec.cargo = '' then 'N/I' else REGEXP_REPLACE(ec.cargo, r'\r|\n', '') end)  as cargo,
    upper(case when ch.cargo is null or ch.cargo = '' then 'N/I' else REGEXP_REPLACE(ch.cargo, r'\r|\n', '') end)  as cargoch, 
    upper(case when ch.cargo_harmonizado is null or ch.cargo_harmonizado = '' then 'N/I' else REGEXP_REPLACE(ch.cargo_harmonizado, r'\r|\n', '') end) as cargo_harmonizado
FROM 
   `bronze_egressos.egressos_consolidado` ec
    LEFT OUTER JOIN `bronze_egressos.cargo_cargo_harmonizado`  ch on
        ec.cargo = ch.cargo
UNION DISTINCT 
SELECT DISTINCT
    upper(case when experience.cargo is null or  experience.cargo = '' then 'N/I' else REGEXP_REPLACE(experience.cargo, r'\r|\n', '') end)  as cargo,
    upper(case when ch.cargo is null or ch.cargo = '' then 'N/I' else REGEXP_REPLACE(ch.cargo, r'\r|\n', '') end)  as cargoch, 
    upper(case when ch.cargo_harmonizado is null or ch.cargo_harmonizado = '' then 'N/I' else REGEXP_REPLACE(ch.cargo_harmonizado, r'\r|\n', '') end) as cargo_harmonizado
FROM 
    `bronze_egressos.linkedin_enriquecidos` AS le
     LEFT OUTER JOIN UNNEST(le.experiences) AS experience
     LEFT OUTER JOIN `bronze_egressos.cargo_cargo_harmonizado`  ch on
         experience.cargo = ch.cargo
UNION DISTINCT 
SELECT DISTINCT
    upper(case when da.cargo_data_source is null or da.cargo_data_source = '' then 'N/I' else REGEXP_REPLACE(da.cargo_data_source, r'\r|\n', '') end)  as cargo,
    upper(case when ch.cargo is null or ch.cargo = '' then 'N/I' else REGEXP_REPLACE(ch.cargo, r'\r|\n', '') end)  as cargoch, 
    upper(case when ch.cargo_harmonizado is null or ch.cargo_harmonizado = '' then 'N/I' else REGEXP_REPLACE(ch.cargo_harmonizado, r'\r|\n', '') end) as cargo_harmonizado
FROM 
    `bronze_egressos.view_dados_abertos` da
    LEFT OUTER JOIN `bronze_egressos.cargo_cargo_harmonizado`  ch on
        da.cargo_data_source = ch.cargo
) A ORDER BY cargo_harmonizado</sql>
